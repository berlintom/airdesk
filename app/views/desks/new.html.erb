<div class="container">
  <h1>Create a new desk</h1>
  <%= simple_form_for [@desk] do |f|%>
  <%= f.input :title %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :photo, as: :file %>
  <div class="btn btn-cta"><%= f.button :submit %></div>
  <% end %>
</div>

<%# new_desk
desks/new %>
